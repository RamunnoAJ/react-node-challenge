{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard-with-typescript",
    "prettier",
    "plugin:jest/recommended"
  ],
  "plugins": ["jest", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": "warn"
  }
}
